    .popup.popup--big.popup-add-legal
      span.popup__close.icon--close.popup-close
      p.popupTitle Добавить юридическое лицо
      form.form.tab-container
        .formTitle
          | Юридическое лицо
          .personEntityTabs
            .personEntityTabsWrapper.tab-link-container
              a.personEntityTabsWrapper__link.personEntityTabsWrapper__link--helper.tab-link.is-active href="#" нет
              a.personEntityTabsWrapper__link.tab-link data-container-class="slide" href="#" да
            p.personEntityTabs__title Юрлицо в российской юрисдикции?
        .tab-content
          .formBlockWrapper
            .formBlock.formBlock--2
              label.fieldWrapper
                span.fieldWrapper__label ФИО генерального директора
                input.fieldWrapper__input type="text" name="" placeholder="Введите ФИО генерального директора"
            .formBlock.formBlock--2
              label.fieldWrapper
                span.fieldWrapper__label ОГРН
                input.fieldWrapper__input type="text" name="" placeholder="Введите ОГРН"
          / .personDataFailure
            span.personDataFailure__icon.icon.icon--notFound
            p.personDataFailure__info Извините, юрлицо не найдено в реестре, мы не можем его добавить
          .formBlock
            label.fieldWrapper
              span.fieldWrapper__label Реквизиты
              textarea.fieldWrapper__input name="" placeholder="Введите банковские реквизиты"
        .tab-content
          .formBlockWrapper
            .formBlock.formBlock--2
              label.fieldWrapper
                span.fieldWrapper__label Название компании
                input.fieldWrapper__input type="text" name="" placeholder="Введите название компании"
            .formBlock.formBlock--2
              label.fieldWrapper
                span.fieldWrapper__label Страна юрисдикции
                select.fieldWrapper__select.form-select placeholder="Выберите страну юрисдикции"
                  option value="" Выберите страну юрисдикции
                  option value="4" Россия
                  option value="1" Германия
                  option value="3" Финляндия
                  option value="5" Кипр
          .formBlock
            label.fieldWrapper
              span.fieldWrapper__label Остальные данные
              textarea.fieldWrapper__textarea name="" placeholder="Введите остальные данные"
          .personRequisites
            a.personRequisites__add.linkButton.linkButton--small.add-requisites-link href="#" добавить реквизиты
            a.personRequisites__remove.remove-requisites-link href="#" не указывать
            .formBlock.add-requisites-hidden
              label.fieldWrapper
                span.fieldWrapper__label Банковские реквизиты
                textarea.fieldWrapper__textarea name="" placeholder="Введите банковские реквизиты"
        .buttonContainer.buttonContainer--center
          button.buttonContainer__button.linkButton.linkButton--small.linkButton--blue type="submit" сохранить
          button.buttonContainer__button.linkButton.linkButton--small.popup-close type="button" отменить



